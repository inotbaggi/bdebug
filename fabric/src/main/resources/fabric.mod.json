{
  "schemaVersion": 1,
  "id": "bdebug",
  "version": "${version}",
  "name": "bDebug",
  "description": "Mod to get Debug information to Minecraft server developer",
  "authors": ["inotbaggi"],
  "contact": {
    "homepage": "https://inotbaggi.t.me"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/bdebug/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.baggi.bdebug.client.BDebug"
    ]
  },
  "mixins": [
    "bdebug.mixins.json",
    {
      "config": "bdebug.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
